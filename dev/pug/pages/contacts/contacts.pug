block variables
    -var breadcrumbsTitle = 'Контакты'
.contacts__inner
    .contacts__content
        ul.contacts__list
            li.contacts__list-item
                a(href="https://goo.gl/maps/kUeBjRiCDRaxhLLT9" target="_blank").contacts__list-link
                    .contacts__list-icon
                        +icon('marker')
                    span.contacts__list-text Hollywood 6519, <br> Los&nbsp;Angeles, CA 90028
            li.contacts__list-item
                a(href="tel:+79000000000").contacts__list-link
                    .contacts__list-icon
                        +icon('phone')
                    span.contacts__list-text +7 900 000-00-00
            li.contacts__list-item
                a(href="mailto:hello@bitmoney.com").contacts__list-link
                    .contacts__list-icon
                        +icon('envelope')
                    span.contacts__list-text hello@bitmoney.com
    .contacts__map
        iframe(src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d425.06797228234433!2d-118.33165842921805!3d34.10187936368344!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x80c2bf3b78405957%3A0xbe95f3c4bb7c28dd!2zNjUxOSBIb2xseXdvb2QgQmx2ZCwgTG9zIEFuZ2VsZXMsIENBIDkwMDI4LCDVhNWr1aHWgdW11aHVrCDVhtWh1bDVodW21aPVttWl1oA!5e0!3m2!1shy!2s!4v1599400281851!5m2!1shy!2s"
            width="100%" height="100%"
            frameborder="0" style="border:0;"
            allowfullscreen="" aria-hidden="false"
            tabindex="0").contacts__map-iframe